package model;

public class Color {

    private String type = "color";
    private String Color;
    private String ip;

    public Color(String color, String ip) {
        this.Color = color;
        this.ip = ip;
    }

    public Color(){}

    public String getType() {
        return type;
    }

    public void setType(String type) {
        this.type = type;
    }

    public String getColor() {
        return Color;
    }

    public void setColor(String color) {
        Color = color;
    }

    public String getIp() {
        return ip;
    }

    public void setIp(String ip) {
        this.ip = ip;
    }
}
